<%- include('partials/head.ejs') %>
    <link rel="stylesheet" href="styles/users.css">
  </head>
  <body>
    <%- include('partials/navigation.ejs') %>
    <main>
        <% if(users.length > 0) { %>
            <ul class="users-list">
                <% for (let user of users) { %>
                    <li class="users-list__item"><%= user %></li>
                <% } %>
            </ul>
        <% } else { %>
            <h1>No users found!</h1>
        <% } %>
    </main>
<%- include('partials/end.ejs') %>
